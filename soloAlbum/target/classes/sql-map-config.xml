<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="photo" type="com.cj.soloAlbum.photo.PhotoVO" />
		<typeAlias alias="user" type="com.cj.soloAlbum.user.UserVO" />
		<typeAlias alias="album" type="com.cj.soloAlbum.album.AlbumVO" />
	</typeAliases>
	
	 <!-- SQL Mapper 설정 -->
	 <mappers>
	 	<mapper resource="mapping/photo-mapping.xml" />
	 	<mapper resource="mapping/user-mapping.xml" />
	 </mappers>
</configuration>